<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryDark">@color/primaryDarkColor</item>
        <item name="colorAccent">@color/secondaryLightColor</item>
        <item name="colorSecondary">@color/secondaryColor</item>
        <item name="android:textColor">@color/secondaryColor</item>
        <item name="android:colorBackground">@color/secondaryTextColor</item>
    </style>

    <style name="LoginTextInputLayoutStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="boxStrokeColor">@color/white</item>
    </style>

    <style name="ShapeAppearanceOverlay.BeduCompras.CornerSize50Percent" parent="">
        <item name="cornerSize">50%</item>
    </style>

</resources>